  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^3.0.3",
    "@vitest/coverage-v8": "^0.32.2",
    "rimraf": "^5.0.1",
